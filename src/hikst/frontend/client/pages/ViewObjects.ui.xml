<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>        
        .eastPanel {
            background-color: grey;
        }
        .westPanel {
            background-color: grey;
        }
        .northPanel {
            background-color: grey;
        }
        .southPanel {
            background-color: grey;
        }
        .centerPanel {
            background-color: grey;
        }       
    </ui:style>
     <g:DockLayoutPanel unit='EM'>
       <g:north size='.4' >
         <g:FlowPanel styleName="{style.northPanel}"/>
       </g:north>
       <g:west size='4.6'>
         <g:FlowPanel styleName="{style.westPanel}"/>
       </g:west>
       <g:center>
         <g:ScrollPanel styleName="{style.centerPanel}" ui:field="centerPanel">
         	<!-- <g:Button width="200px" text="Lag Ett Nytt Objekt" ui:field="buttonSave"/> -->
         	<g:FlexTable ui:field="flexyTable"/>

         </g:ScrollPanel>
       </g:center>
       <g:east size='15' unit='EM'>
         <g:FlowPanel styleName="{style.eastPanel}">

         </g:FlowPanel>
       </g:east>     
        <g:south size="6.1" unit='EM'>
         <g:FlowPanel styleName="{style.southPanel}">

         </g:FlowPanel>
        </g:south>       
     </g:DockLayoutPanel>
</ui:UiBinder>