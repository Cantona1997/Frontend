<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>        
        .eastPanel {
            background-color: grey;
        }
        .westPanel {
            background-color: grey;
        }
        .northPanel {
            background-color: grey;
        }
        .southPanel {
            background-color: grey;
        }
        .centerPanel {
            background-color: white;
        }       
    </ui:style>
     <g:DockLayoutPanel unit='EM' width="1024" height="768">
        <g:north size='3.0'>
         <g:FlowPanel styleName="{style.northPanel}">
         	<g:Button ui:field="adminAccount" width="150px">Administrere Konto</g:Button>
         	<g:Button ui:field="emailAdmin" width="150px">Email Administrator</g:Button>
         	<g:Button ui:field="userList" width="150px">Brukerliste</g:Button>
         	<g:Button ui:field="buttonLogout" width="150px">Logg Ut</g:Button>
         </g:FlowPanel>
       </g:north>
       <g:west size='13.5'>
         <g:FlowPanel styleName="{style.westPanel}">
         	<g:Label/>
         	<g:Label text="Create New Object" horizontalAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Name" ui:field="name" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Effect" ui:field="effect" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Voltage" ui:field="voltage" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Current" ui:field="current" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Latitude" ui:field="latitude" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Longitude" ui:field="longitude" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Start temperature" ui:field="self_temperature" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Target temperature" ui:field="target_temperature" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Base area" ui:field="base_area" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Base height" ui:field="base_height" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:TextBox text="Heat loss rate" ui:field="heat_loss_rate" width="156px" textAlignment="ALIGN_CENTER"/>
         	<g:Button width="150px" ui:field="back">Tilbake</g:Button>
         	<g:Button ui:field="saveObject" width="140px">Lagre Objekt</g:Button>
         </g:FlowPanel>
       </g:west>
       <g:center size='25.5'>
         <g:FlowPanel styleName="{style.centerPanel}" ui:field="centerPanel">

         </g:FlowPanel>
       </g:center>
       <g:east size='13.5'>
         <g:FlowPanel styleName="{style.eastPanel}" ui:field="eastPanel">
         	<g:Button width="150px" ui:field="addChildObject">Add Child Object</g:Button>
         	<g:Button width="150px" ui:field="addUsagePattern">Add Usage Pattern</g:Button>
         	<g:Button width="149px" text="Vis Kart" ui:field="showMap"/>
         	<g:AbsolutePanel height="280px" ui:field="mapsPanel" width="328px"/>
         </g:FlowPanel>
       </g:east>     
        <g:south size="12.6">
         <g:FlowPanel styleName="{style.southPanel}"/>
        </g:south>       
     </g:DockLayoutPanel>
</ui:UiBinder> 