<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
 xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>        
        .eastPanel {
            background-color: grey;
        }
        .westPanel {
            background-color: grey;
        }
        .northPanel {
            background-color: grey;
        }
        .southPanel {
            background-color: grey;
        }
        .centerPanel {
            background-color: grey;
        }    
	</ui:style>
	 <g:DockLayoutPanel unit='EM' width="1024" height="768">
	 	<g:north size='7.3'>
			<g:FlowPanel styleName="{style.northPanel}">
				<g:Button text="Lag nytt Usage Pattern" width="154px" ui:field="createNewButton"/>
				<g:Button text="Tilbake" ui:field="tilbakeButton"/>
			</g:FlowPanel>
		</g:north>
		<g:west size='11.4'>
			<g:FlowPanel styleName="{style.westPanel}">
				<g:ListBox width="120px" height="69px" ui:field="impactFactorType"/>
			</g:FlowPanel>
		</g:west>
		<g:south size="7.7">
			<g:FlowPanel styleName="{style.southPanel}">
			</g:FlowPanel>
		</g:south>
		<g:east size='9.6'>
			<g:FlowPanel styleName="{style.eastPanel}"/>
		</g:east>
				<g:center size='10.0'>
			<g:ScrollPanel styleName="{style.centerPanel}" ui:field="scrollPanel">
				<g:FlexTable width="100%" height="346px" ui:field="FactorTable"/>
			</g:ScrollPanel>
		</g:center>
	 </g:DockLayoutPanel>
</ui:UiBinder> 
   